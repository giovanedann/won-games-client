{
  "name": "won-games",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=1",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "check": "yarn upgrade-interactive --latest",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --collect-coverage",
    "test:e2e": "cypress open",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node ./src/utils/convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "generate-gql-types": "yarn schema:download && yarn schema:convert && yarn codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@types/lodash": "^4.14.194",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.0.3",
    "framer-motion": "^10.12.9",
    "fs-extra": "^11.1.1",
    "graphql": "^15.5.0",
    "joi": "^17.9.2",
    "lodash": "^4.17.21",
    "next": "13.4.1",
    "next-auth": "^4.22.1",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.0.0",
    "nextjs-progressbar": "^0.0.16",
    "polished": "^4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-slick": "^0.29.0",
    "styled-components": "^5.3.10"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-typescript": "^7.21.5",
    "@storybook/addon-actions": "^7.0.9",
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-interactions": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/addon-mdx-gfm": "^7.0.9",
    "@storybook/addon-viewport": "^7.0.9",
    "@storybook/nextjs": "^7.0.9",
    "@storybook/react": "^7.0.9",
    "@storybook/testing-library": "^0.1.0",
    "@stripe/react-stripe-js": "^2.1.0",
    "@stripe/stripe-js": "^1.52.1",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "18.16.3",
    "@types/react": "18.2.6",
    "@types/react-slick": "^0.23.10",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "apollo": "^2.34.0",
    "babel-loader": "^9.1.2",
