{
  "name": "won-games",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prepare": "husky install",
    "test": "jest --verbose",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --collect-coverage",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node ./src/utils/convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "generate-gql-types": "yarn schema:download && yarn schema:convert && yarn codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@types/lodash": "^4.14.191",
    "framer-motion": "^10.0.0",
    "graphql": "^15.5.0",
    "lodash": "^4.17.21",
    "next": "13.2.4",
    "next-pwa": "^5.6.0",
    "polished": "^4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-slick": "^0.29.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^7.0.2",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.5",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-slick": "^0.23.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "apollo": "^2.34.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^13.1.0",
    "plop": "^3.1.1",
    "prettier": "2.8.4",
    "react-icons": "^4.7.1",
    "storybook-addon-next": "^1.7.1",
    "styled-media-query": "^2.1.2",
    "typescript": "4.9.5",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "webpack": "*"
  }
}
